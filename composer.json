{
    "name": "bobdenotter/passwordprotect",
    "description": "A small extension to password protect pages on your site, when using {{ passwordprotect() }} in your templates.",
    "type": "bolt-extension",
    "keywords": [
        "password",
        "protect"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bobdenotter\\PasswordProtect\\": "src/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bobdenotter\\PasswordProtect\\PasswordProtectExtension"
    }
}
